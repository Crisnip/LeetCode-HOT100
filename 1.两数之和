class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i;
        int j;
        int []numbers=new int[2];
        for(i=0;i<nums.length-1;i++)
        {
            for(j=i+1;j<nums.length;j++)
            {
                if(nums[i]+nums[j]==target)
                {
                    numbers[0]=i;
                    numbers[1]=j;
                }
            }
        }
        return numbers;
    }
}
